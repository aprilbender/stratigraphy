{
  "name": "stratigraphy-svg-exports",
  "version": "0.0.1",
  "description": "SVG assets that you won't be interested in",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compress:paleogeography": "npx svgo 'Paleogeography_Maps.svg' -o 'Paleogeography_Maps_Compressed.svg'",
    "compress:paleogeology": "npx svgo 'Paleogeology_Maps.svg' -o 'Paleogeology_Maps_Compressed.svg'",
    "compress:paintings": "npx svgo 'Paintings.svg' -o 'Paintings_Compressed.svg'",
    "compress:fossils": "npx svgo 'Fossils.svg' -o 'Fossils_Compressed.svg'",
    "compress:strata": "npx svgo 'Strata.svg' -o 'Strata_Compressed.svg'",
    "compress:coal": "npx svgo 'Coal.svg' -o 'Coal_Compressed.svg'",
    "compress:gas": "npx svgo 'Gas.svg' -o 'Gas_Compressed.svg'",
    "compress:oil": "npx svgo 'Oil.svg' -o 'Oil_Compressed.svg'",
    "compress:zircons": "npx svgo 'Zircons.svg' -o 'Zircons_Compressed.svg'",
    "compress:strat_chart": "npx svgo 'Strat Chart.svg' -o 'Strat_Chart_Compressed.svg'",
    "compress:strat_panel_key": "npx svgo 'Key Map_strat panels.svg' -o 'Key_Map_strat_panels_Compressed.svg' --pretty --enable=prefixIds --disable=convertStyleToAttrs --disable=inlineStyles",
    "compress:utah_to_denver": "npx svgo 'Central Utah to Denver Section.svg' -o 'Utah_to_Denver_Compressed.svg' --pretty --enable=prefixIds --disable=convertStyleToAttrs --disable=inlineStyles",
    "compress:roadway": "npx svgo 'Key Map_Roadway Sections.svg' -o 'Key Map_Roadway Sections_Compressed.svg' --pretty --enable=prefixIds --disable=convertStyleToAttrs --disable=inlineStyles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aprilbender/stratigraphy.git"
  },
  "author": "April Johnson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aprilbender/stratigraphy/issues"
  },
  "homepage": "https://github.com/aprilbender/stratigraphy#readme",
  "dependencies": {
    "svgo": "^3.0.2"
  }
}
